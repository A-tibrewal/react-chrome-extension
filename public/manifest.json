{
  "short_name": "My Extension",
  "name": "My Extension",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["app/jQuery.js","app/background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
   "128": "icon128.png" 
  },
  "content_scripts" : [
    {
      "matches": [ "<all_urls>" ],
      "css": ["/static/css/app.css"],
      "js": ["/static/js/content.js"]
    }
  ],
  "web_accessible_resources":[
    "/static/css/content.css"
  ],
  "permissions": [
    "contextMenus"
  ]
}
